<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.AccountMapperA">

    <update id="updateDollar" parameterType="org.example.entity.Account">
        UPDATE dollar SET money = #{money}  WHERE user_id=#{userId}
    </update>

    <update id="updateRMB" parameterType="org.example.entity.Account">
        UPDATE rmb SET money = #{money}  WHERE user_id=#{userId}
    </update>

</mapper>